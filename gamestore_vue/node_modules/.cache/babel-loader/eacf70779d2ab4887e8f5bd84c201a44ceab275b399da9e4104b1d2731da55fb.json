this.getLatestProducts()\n\n    document.title = ' Homepage | Game Item Shop!'\n  },\n  methods: {\n    async getLatestProducts(){\n      this.$store.commit('setIsLoading', true)\n\n      await axios\n      .get('/api/v1/latest-products/')\n      .then(response => {\n        this.latestProducts = response.data\n      })\n      .catch(error => {\n        console.log(error)\n      })\n\n      this.$store.commit('setIsLoading', false)\n    }\n  }\n}\n</script>"],"mappings":"AA6BA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,UAAS,MAAO,yBAAwB;AAE/C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAM;MACJC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,iBAAiB,CAAC;IAEvBC,QAAQ,CAACC,KAAI,GAAI,6BAA4B;EAC/C,CAAC;EACDC,OAAO,EAAE;IACP,MAAMH,iBAAiBA,CAAA,EAAE;MACvB,IAAI,CAACI,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMZ,KAAI,CACTa,GAAG,CAAC,0BAA0B,EAC9BC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACX,cAAa,GAAIW,QAAQ,CAACZ,IAAG;MACpC,CAAC,EACAa,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB,CAAC;MAED,IAAI,CAACN,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC1C;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}