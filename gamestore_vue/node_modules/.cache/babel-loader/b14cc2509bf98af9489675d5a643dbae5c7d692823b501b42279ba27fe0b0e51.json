ios from 'axios'\r\nexport default {\r\n    name: 'Product',\r\n    data(){\r\n        return{\r\n            product: {},\r\n            quantity: 1\r\n\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getProduct()\r\n    },\r\n    method: {\r\n        getProduct(){\r\n            const category_slug = this.$route.params.category_slug\r\n            const product_slug = this.$route.params.product_slug\r\n\r\n            axios\r\n                .get('/api/v1/products/${category_slug}/${product_slug}')\r\n                .then(response => {\r\n                    this.product = response.data\r\n                })\r\n                .catch(error =>{\r\n                    console.log(error)\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AAiCA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,OAAO,EAAE,CAAC,CAAC;MACXC,QAAQ,EAAE;IAEd;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,UAAU,CAAC;EACpB,CAAC;EACDC,MAAM,EAAE;IACJD,UAAUA,CAAA,EAAE;MACR,MAAME,aAAY,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,aAAY;MACrD,MAAMG,YAAW,GAAI,IAAI,CAACF,MAAM,CAACC,MAAM,CAACC,YAAW;MAEnDX,KAAI,CACCY,GAAG,CAAC,mDAAmD,EACvDC,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACX,OAAM,GAAIW,QAAQ,CAACZ,IAAG;MAC/B,CAAC,EACAa,KAAK,CAACC,KAAI,IAAI;QACXC,OAAO,CAACC,GAAG,CAACF,KAAK;MACrB,CAAC;IACT;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}